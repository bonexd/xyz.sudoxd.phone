<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pixel2">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="xyz.sudoxd.phone.pixel2">

        <TextView
            android:id="@+id/textView28"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Google Pixel 2"
            android:textAlignment="textStart"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Release: October, 2017\nSIM Type: Nano-SIM and eSIM\nDisplay: 5.0' 1080x1920 AMOLED, Gorilla Glass 5\nStock Android Versions: 8.0 - 10.0 - Stock Android\nCPU: Qualcomm Snapdragon 835 (4x2.35 GHz Kryo and 4x1.9 GHz Kryo)\nGPU: Adreno 540\nRAM: 4GB\nStorage: 64GB, 128GB No MicroSD Slot\nRear Camera: 12.2MP (f/1.8) OIS, PDAF and laser AF, dual-LED flash, HDR, Panorama\n- Video: 2160p30, 1080p30/60/120, 720p240\nFront Camera: 8MP (f/2.4)\nBattery: Non User Replacable 2770mAh Li-Ion\nHeadphone Jack: No\nFingerprint Sensor: Yes, Back Casing\nCharging Port: Type-C"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView28" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Google Pixel 2 XL"
            android:textAlignment="textStart"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView29" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="Release: October, 2017\nSIM Type: Nano-SIM and eSIM\nDisplay: 6.0' 1440x2880 P-OLED, Gorilla Glass 5\nStock Android Versions: 8.0 - 10.0 - Stock Android\nCPU: Qualcomm Snapdragon 835 (4x2.35 GHz Kryo and 4x1.9 GHz Kryo)\nGPU: Adreno 540\nRAM: 4GB\nStorage: 64GB, 128GB No MicroSD Slot\nRear Camera: 12.2MP (f/1.8) OIS, PDAF and laser AF, dual-LED flash, HDR, Panorama\n- Video: 2160p30, 1080p30/60/120, 720p240\nFront Camera: 8MP (f/2.4)\nBattery: Non User Replacable 3520mAh Li-Ion\nHeadphone Jack: No\nFingerprint Sensor: Yes, Back Casing\nCharging Port: Type-C"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <Button
            android:id="@+id/button7"
            android:layout_width="0dp"
            android:layout_height="46dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/roboto_medium"
            android:onClick="gotopixel2webpage"
            android:text="Manufacturer Webpage"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>