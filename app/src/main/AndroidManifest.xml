<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.sudosudoo.sudoproductions">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".rom"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".onem7"
            android:parentActivityName=".htconeseries" />
        <activity
            android:name=".htcmain"
            android:parentActivityName=".rom" />
        <activity
            android:name=".onem7rom"
            android:parentActivityName=".onem7" />
        <activity
            android:name=".htconeseries"
            android:parentActivityName=".htcmain" />
        <activity
            android:name=".onem7romll"
            android:parentActivityName=".onem7rom" />
        <activity
            android:name=".onem7rommm"
            android:parentActivityName=".onem7rom" />
        <activity
            android:name=".onem7romnougat"
            android:parentActivityName=".onem7rom" />
        <activity
            android:name=".onem7romoreo"
            android:parentActivityName=".onem7rom" />
        <activity
            android:name=".onem8"
            android:parentActivityName=".htconeseries" />
        <activity
            android:name=".onem8rom"
            android:parentActivityName=".onem8" />
        <activity
            android:name=".onem8romll"
            android:parentActivityName=".onem8rom" />
        <activity
            android:name=".onem8rommm"
            android:parentActivityName=".onem8rom" />
        <activity
            android:name=".onem8rommmse"
            android:parentActivityName=".onem8rom" />
        <activity android:name=".onem8romnoug"></activity>
    </application>

</manifest>